Image\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Gradient\" name=\"gradientBg\" checked condition=\"bg.type === 'color'\">\n        <input type=\"range\" inline name=\"gradientAngle\" title=\"Gradient Angle\" min=\"0\" max=\"360\" value=\"90\" step=\"1\" condition=\"bg.type === 'color' && gradientBg\">\n        <input type=\"color\" name=\"gradientColor\" title=\"Gradient Color\" value=\"#e67de3\" condition=\"bg.type === 'color' && gradientBg\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#562F69\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.1\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"mt-auto mb-auto\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12 {{contentAlign}} d-flex flex-wrap\">\n                <h1 class=\"mbr-section-title mbr-fonts-style align-left w-100\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\">35+ Χρόνια<b>&nbsp;</b></h1>\n                <div class=\"img-wrapper\">\n                    <img src=\"@PROJECT_PATH@/assets/images/5dmk3-106-3838-1320x880.jpg\" alt=\"Mobirise Website Builder\">\n                </div>\n                <h2 class=\"mbr-section-subtitle mbr-fonts-style mb-0 align-right w-100\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-1\" mbr-if=\"showSubtitle\"><br>Χορός</h2>\n            </div>\n      