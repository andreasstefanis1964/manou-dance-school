lbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\" mbr-buttons mbr-theme-style=\"display-4\"><a href class=\"btn btn-info item-btn\" target=\"_blank\" data-app-placeholder=\"Type Text\">\n                                Click here\n                            </a></div>\n                    </div>\n                </div>\n            </div>\n\n        </div>\n    </div>\n</section>",
